{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",

  "mainEntryPointFile": "lib/index.d.ts",

  "apiReport": {
    "enabled": true,
  },

  "dtsRollup": {
    "enabled": true,

    "untrimmedFilePath": "./dist/internal/<unscopedPackageName>.d.ts"
  },

  "docModel": {
    "enabled": true
  },

  "testMode": true
}
